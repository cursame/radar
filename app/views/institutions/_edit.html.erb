<%= form_tag("/institutions/update?id=#{institution.id}") do -%>
	<%= text_field_tag 'name', institution.name%>
	<%= text_field_tag 'url', institution.url%>
	<%= text_field_tag 'phone', institution.phone%>
	<%= text_field_tag 'extencion', institution.extencion %>
	<%= submit_tag 'Guardar'%>
	<%= link_to 'Cancelar', cancel_institution_path(id: institution.id), remote: true%>
<%end%>
