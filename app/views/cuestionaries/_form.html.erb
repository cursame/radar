<%= form_for @cuestionary do |f|%>
<div class="title">
  <%= content_tag(:h4, "Preguntas")%>      
</div>
<div id="" class="questions_container">
  <%= f.fields_for :questions do |builder|%>
  <%= render  'question_fields', f: builder %>
  <%end%>
</div>

<div class="add">
	<%= link_to_add_fields "Agregar otra pregunta", f, :questions, '#questions_container' %>
</div>


<span class="btn btn-embed ">
	<%= f.submit %>
</span>
<%end%>
